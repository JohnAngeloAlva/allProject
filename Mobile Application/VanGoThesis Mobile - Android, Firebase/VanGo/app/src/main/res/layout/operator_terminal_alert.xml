<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFF">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="416dp"
        android:layout_height="155dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle"
        app:tint="@color/yellow_500"/>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/head_logo" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="271dp"
        android:layout_height="205dp"
        android:layout_marginTop="38dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        app:srcCompat="@drawable/uv" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">



    </androidx.appcompat.widget.LinearLayoutCompat>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head_title">

        <Button
            android:id="@+id/sm"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_margin="20dp"
            android:layout_weight="1"
            android:background="@drawable/button"
            android:fontFamily="@font/montserratbold"
            android:text="SM"
            android:textColor="@color/white"
            app:backgroundTint="@color/choose_user" />

        <Button
            android:id="@+id/talipapa"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_margin="20dp"
            android:layout_weight="1"
            android:background="@drawable/button"
            android:fontFamily="@font/montserratbold"
            android:text="Talipapa "
            android:textColor="@color/white"
            app:backgroundTint="@color/choose_user" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <Button
            android:id="@+id/pacific"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_margin="20dp"
            android:layout_weight="1"
            android:background="@drawable/button"
            android:fontFamily="@font/montserratbold"
            android:text="Pacific"
            android:textColor="@color/white"
            app:backgroundTint="@color/choose_user" />

        <Button
            android:id="@+id/grand"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_margin="20dp"
            android:layout_weight="1"
            android:background="@drawable/button"
            android:fontFamily="@font/montserratbold"
            android:text="Grand"
            android:textColor="@color/white"
            app:backgroundTint="@color/choose_user" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/head_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="190dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:gravity="center">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsregular"
            android:text="Choose"
            android:textColor="@color/black"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat" />

        <TextView
            android:id="@+id/textViews"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text=" Terminal"
            android:textColor="@color/yellow_500"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat" />

    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>